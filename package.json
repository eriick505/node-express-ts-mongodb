{
  "type": "commonjs",
  "name": "rest-api",
  "version": "1.0.0",
  "description": "Estudo de REST API com nodejs e mongodb",
  "main": "src/server.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "postinstall": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/server.js"
  },
  "author": "Erick <santoserick9@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "nodemon": "^2.0.12",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.6.6",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.6.2"
  }
}
